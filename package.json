{
  "name": "eslint-config-intense",
  "version": "0.7.7",
  "description": "ESLint rules for the overly serious",
  "repository": "dimitropoulos/eslint-config-intense",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslint-config",
    "eslintconfig",
    "config",
    "airbnb",
    "typescript",
    "jest",
    "ramda",
    "functional",
    "javascript",
    "styleguide"
  ],
  "author": {
    "name": "Dimitri Mitropoulos",
    "url": "https://github.com/dimitropoulos"
  },
  "maintainers": [
    "dimitropoulos"
  ],
  "bugs": {
    "url": "https://github.com/dimitropoulos/eslint-config-intense/issues"
  },
  "homepage": "https://github.com/dimitropoulos/eslint-config-intense",
  "engines": {
    "node": ">=16"
  },
  "main": "index.js",
  "scripts": {
    "build": "node build.js",
    "prefind-deprecated": "yarn build",
    "find-deprecated": "eslint-find-rules --deprecated index.js",
    "prefind-unused": "yarn build",
    "find-unused": "eslint-find-rules --unused index.js",
    "predeploy": "yarn build",
    "deploy": "yarn publish",
    "lint": "eslint --ext=tsx --ext=ts --ext=mjs --ext=js --ext=jsx ."
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "~4.29.1",
    "@typescript-eslint/parser": "~4.29.1",
    "eslint": "~7.32.0",
    "eslint-plugin-eslint-comments": "~3.2.0",
    "eslint-plugin-import": "~2.24.0",
    "eslint-plugin-jest": "~24.4.0",
    "eslint-plugin-jest-formatting": "~3.0.0",
    "eslint-plugin-no-secrets": "~0.8.9",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-ramda": "~2.5.1",
    "eslint-plugin-react": "~7.24.0",
    "eslint-plugin-react-hooks": "~4.2.0",
    "eslint-plugin-simple-import-sort": "~7.0.0",
    "typescript": "~4.3.5"
  },
  "devDependencies": {
    "@types/confusing-browser-globals": "^1.0.0",
    "@types/eslint": "^7.28.0",
    "@types/node": "^16.6.1",
    "eslint-config-helpers": "^1.1.0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "~4.29.1",
    "@typescript-eslint/parser": "~4.29.1",
    "confusing-browser-globals": "~1.0.10",
    "eslint": "~7.32.0",
    "eslint-find-rules": "~3.6.1",
    "eslint-plugin-eslint-comments": "~3.2.0",
    "eslint-plugin-import": "~2.24.0",
    "eslint-plugin-jest": "~24.4.0",
    "eslint-plugin-jest-formatting": "~3.0.0",
    "eslint-plugin-no-secrets": "~0.8.9",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-ramda": "~2.5.1",
    "eslint-plugin-react": "~7.24.0",
    "eslint-plugin-react-hooks": "~4.2.0",
    "eslint-plugin-simple-import-sort": "~7.0.0",
    "typescript": "~4.3.5"
  }
}
