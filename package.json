{
  "name": "eslint-config-intense",
  "version": "0.8.1",
  "description": "ESLint rules for the overly serious",
  "repository": "dimitropoulos/eslint-config-intense",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslint-config",
    "eslintconfig",
    "config",
    "airbnb",
    "typescript",
    "jest",
    "ramda",
    "functional",
    "javascript",
    "styleguide"
  ],
  "author": {
    "name": "Dimitri Mitropoulos",
    "url": "https://github.com/dimitropoulos"
  },
  "maintainers": [
    "dimitropoulos"
  ],
  "bugs": {
    "url": "https://github.com/dimitropoulos/eslint-config-intense/issues"
  },
  "homepage": "https://github.com/dimitropoulos/eslint-config-intense",
  "engines": {
    "node": ">=18"
  },
  "main": "index.js",
  "scripts": {
    "build": "node build.js",
    "prefind-deprecated": "yarn build",
    "find-deprecated": "eslint-find-rules --deprecated index.js",
    "prefind-unused": "yarn build",
    "find-unused": "eslint-find-rules --unused index.js",
    "predeploy": "yarn build",
    "deploy": "yarn publish",
    "lint": "eslint --ext=tsx --ext=ts --ext=mjs --ext=js --ext=jsx ."
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "~5.59.1",
    "@typescript-eslint/parser": "~5.59.1",
    "eslint": "~8.39.0",
    "eslint-plugin-eslint-comments": "~3.2.0",
    "eslint-plugin-import": "~2.27.5",
    "eslint-plugin-jest": "~27.2.1",
    "eslint-plugin-jest-formatting": "~3.1.0",
    "eslint-plugin-no-secrets": "~0.8.9",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-ramda": "~2.5.1",
    "eslint-plugin-react": "~7.32.2",
    "eslint-plugin-react-hooks": "~4.6.0",
    "eslint-plugin-simple-import-sort": "~10.0.0",
    "typescript": "~5.0.4"
  },
  "devDependencies": {
    "@types/confusing-browser-globals": "^1.0.0",
    "@types/eslint": "8.37.0",
    "@types/node": "18.16.1",
    "eslint-find-rules": "4.1.0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "5.59.1",
    "@typescript-eslint/parser": "5.59.1",
    "confusing-browser-globals": "1.0.11",
    "eslint": "8.39.0",
    "eslint-config-helpers": "1.2.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-no-secrets": "0.8.9",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-ramda": "2.5.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "typescript": "5.0.4"
  }
}
